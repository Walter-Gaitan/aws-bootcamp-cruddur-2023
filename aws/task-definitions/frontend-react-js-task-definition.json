{
    "family": "frontend-react-js",
    "executionRoleArn": "arn:aws:iam::596027898727:role/CruddurServiceExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "frontend-react-js",
            "image": "596027898727.dkr.ecr.us-east-2.amazonaws.com/frontend-react-js:latest",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "REACT_APP_API_URL",
                    "value": "http://[public-ip]:4567/api"
                }
            ]
        }
    ]
}